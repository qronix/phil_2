<div id='userPanel'>
    <div id='usertoolspanel'>
        
    </div>
    <div id='tableHeader'>
        <span>Username</span>
        <span>Name</span>
        <span>Email</span>
        <span>Role</span>
    </div>
    {{# each users}}
    <div class="userRow">
        <span>{{this.username}}<div class="userInfo"><a href="/users/{{this.id}}">Edit</a>|<a href="#">Permissions</a></div></span>
        <span>{{this.name}}</span>
        <span>{{this.email}}</span>
        <span>{{this.role}}</span> 
    </div>
    {{/each}}
</div>